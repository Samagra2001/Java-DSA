import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.print("Enter first value: ");
	int a = sc.nextInt();
    System.out.print("Enter second value: ");
    int b = sc.nextInt();
    int sum = a+b;
	System.out.println("Sum of "+a +" and " + b+" is "+ sum);
	}
}
